{
  "name": "rock-paper-scissors",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node app.js",
    "build": "browserify src/index.js -o public/bundle.js -t [ babelify --presets env ] ",
    "watch": "watchify src/index.js -o public/bundle.js -v -t [ babelify --presets env ] ",
    "test": "nyc mocha --timeout 10000 --require babel-polyfill --require babel-register",
    "format": "prettier --list-different \"src/*.js\"",
    "lint": "eslint \"src/*.js\" --quiet",
    "detect": "jscpd -f \"src/*.js\" -e \"**/node_modules/**\""
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint && npm run detect && npm run test"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^1.0.0-rc.13",
    "jscpd": "^0.6.22",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "prettier": "^1.14.2",
    "puppeteer": "^1.7.0",
    "watchify": "^3.11.0"
  }
}
